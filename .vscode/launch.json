  "version": "0.2.0",
  "configurations": [
    {
      "name": "flutterdata",
      "request": "launch",
      "type": "dart",
      "toolArgs": [
        "--dart-define",
        "HOST=goalbase.herokuapp.com",
        "--dart-define",
        "HOST_PREFIX=api/v1",
        "--dart-define",
        "SCHEME=https"
      ]
    },
    {
      "name": "flutterdata (profile mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile"
    },
    {
      "name": "flutterdata (release mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release"
    }
  ]
}
